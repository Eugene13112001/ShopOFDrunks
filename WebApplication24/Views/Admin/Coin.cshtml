@{
    ViewData["Title"] = "Redirect";
    Layout = null;
}
<style>
    img {
        height: auto;
        width: auto;
        display: flex;
        flex-grow: 3;
    }

    body {
        background-color: green;
        padding: 0px;
        border: 0px;
        margin: 0px;
    }

    .products {
        display: flex;
        text-align: center;
        min-height: 100px;
        flex-wrap: wrap;
    }

    .product {
        background-color: yellow;
        box-sizing: border-box;
        display: flex;
        margin: 7px;
        min-height: 250px;
        min-width: 250px;
        flex-direction: column;
    }

    .lab {
        display: flex;
        flex-grow: 10;
    }

    .coins {
        display: flex;
    }

    .coin {
        margin: 7px;
    }

    .korzina {
        display: flex;
        margin-left: 8px;
        width: auto;
        flex-direction: column;
    }

    .panelcoins {
        display: flex;
        width: auto;
        margin-left: 8px;
        flex-direction: column;
    }

    .buybutton {
        color: blue;
        background-color: red;
        font-size: 16px;
        text-align: center;
        padding: 12px 28px;
        border-radius: 8px;
    }

    .addbtn {
        color: blue;
        background-color: red;
        font-size: 13px;
        text-align: center;
        padding: 9px 12px;
        margin: auto;
    }

    img {
        max-width: 107px;
        max-height: 107px;
    }

    .main {
        display: flex;
        flex-direction: column;
        font-size: 120%;
    }

    .paneladdd {
        background-color: aqua;
    }

    .mainl {
        display: flex;
        flex-direction: column;
        align-items: center;
        font-size: 120%;
    }

    .error {
        font-size: 30px;
        color: red;
    }

    .addbtn {
        display: block;
        color: blue;
        background-color: red;
        font-size: 13px;
        text-align: center;
        font-size: 130%;
        padding: 9px 12px;
        margin: auto;
    }

    .coin {
        display: flex;
    }

    .coinlb {
        display: flex;
        align-items: center;
        flex-direction: column;
    }
</style>
<body>

    <div id="content"></div>

    <script crossorigin src="https://cdnjs.cloudflare.com/ajax/libs/react/16.11.0/umd/react.development.js"></script>
    <script crossorigin src="https://cdnjs.cloudflare.com/ajax/libs/react-dom/16.11.0/umd/react-dom.development.js"></script>
    <script src="~/js/AdminCoins.jsx"></script>


</body>